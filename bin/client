#!/bin/sh

CP=.:lib-client:lib-client/*

NORMAL=
SPECIAL=

while [ $# -gt 0 ]
do

        HEADER=`echo $1 | head --bytes=2`

        case "$HEADER" in
        -D)
                SPECIAL="$SPECIAL $1"
                ;;
        *)
                NORMAL="$NORMAL $1"
                ;;
        esac
        shift
done

java -Xms128M -Xmx768M -cp $CP -Djava.library.path=external/native_libraries -Dgat.adaptor.path=deploy/adaptors $SPECIAL ibis.dog.client.gui.GUI $NORMAL



